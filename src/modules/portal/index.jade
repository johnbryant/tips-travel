.portal
    #toggle.button-container(ng-click='openNav()')
        span(ng-class='{top_active:topactive}')
        span(ng-class='{middle_active:middleactive}')
        span(ng-class='{bottom_active:bottomactive}')
    #overlay(ng-class='{overlay:onclose,overlay_open:onopen}')
        nav
            ul
                li
                    a(ng-click='goSignin()') Sign in
                li
                    a(href='#') Sign up
                li
                    a(ng-click='goSearch()') Search

    // Start page content
    header.container-fluid.intro-lg.uppercase.bkg
        .col-xs-10.col-xs-offset-1.col-sm-8.col-sm-offset-2.col-md-8.col-md-offset-2
            h1#title.animated.rubberBand TipsTravel.
            h3#sub.animated.bounceInLeft {{ tips }}
            form.form-signin(
                name='registerForm'
            )
                label.sr-only(for='inputEmail') Email address
                input.form-control(
                  ng-model='email'
                  type='email'
                  placeholder='Email address'
                  required=''
                  autofocus=''
                  )
                label.sr-only(for='inputUsername') Username
                input.form-control(
                  type='text'
                  placeholder='Username'
                  required=''
                  ng-model='username'
                  ng-minlength = '6'
                  )
                label.sr-only(for='inputPassword') Password
                input.form-control(
                  type='password'
                  placeholder='Password'
                  required=''
                  ng-model='password'
                  ng-minlength='6'
                  )
                p.registerInfo {{ errorInfo }}
                button.btn.btn-lg.btn-primary.btn-block(
                  ng-click='signup()'
                  ) Sign up

